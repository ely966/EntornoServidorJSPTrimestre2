package com.example.demo.service;

import org.springframework.beans.factory.annotation.Autowired;

import com.example.demo.model.Ped_Prod_Cantida;
import com.example.demo.model.Pedido;
import com.example.demo.model.Producto;


public interface Ped_Prod_CantidaService {

	public void init();
	public Ped_Prod_Cantida add(Pedido pedido, Producto producto, int cantidad);
	public Ped_Prod_Cantida borrar(Pedido pedido, Producto producto, int cantidad);
	public Ped_Prod_Cantida modificar(Pedido pedido, Producto producto, int cantidad);
}
